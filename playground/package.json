    "start": "node --enable-source-maps ./src/server.mjs"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@marko/express": "^2.1.0",
    "@marko/webpack": "^10.0.1",
    "@rsbuild/core": "^1.0.10",
    "@rspack/core": "^1.0.10",
    "express": "^4.21.1",
    "rsbuild-plugin-marko": "^2.2.1"
  },
  "devDependencies": {
    "postcss-load-config": "^6.0.1"
  }
}
